import java.util.*;

public class a1_q7 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int inv = 0;
        int op = 1;
        while (n > 0) {
            int od = n % 10;
            int ip = od;
            int id = op;
            n = n / 10;
            inv = inv + id * (int) Math.pow(10, ip - 1);
            op++;

        }
        System.out.println(inv);

    }

}
