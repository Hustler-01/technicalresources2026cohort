import java.util.*;

public class a1_q5 {
    public static void main(String[] aStrings) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int d = 1;
        while (n >= 10) {
            n = n / 10;
            d++;
        }
        System.out.println(d + " digits");
    }
}
